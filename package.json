    "lodash": "^4.17.21",
    "node-sass": "4.14.1",
    "sass-loader": "10.1.1",
    "vue": "^2.6.14",
    "vue-devtools": "^5.1.4",
    "vue-i18n": "^8.26.7",
    "vue-router": "^3.4.5",
    "vuex": "^3.5.1",
    "vuex-electron": "^1.0.3",
    "vuex-pathify": "^1.4.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "~4.42.1"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "electron-config": "^1.0.0",
    "element-ui": "^2.15.6",
    "express": "^4.17.1",
